{
  "name": "utcaa-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "tt": "babel-node ./tt.js",
    "start": "nodemon --exec babel-node server/server.js",
    "db:config": "babel-node ./db/scripts/generateSequelizeCLIConfig.js",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "test:integration": "mocha --compilers js:@babel/register tests/integration/*/*.test.js",
    "test:unit": "mocha --compilers js:@babel/register tests/unit/*/*.test.js",
    "test": "mocha --compilers js:@babel/register tests/*/*/*.test.js",
    "lint": "eslint ./",
    "config:prod": "./configure-prod.sh"
  },
  "dependencies": {
    "aws-sdk": "2.429.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "cuid": "2.1.6",
    "express": "4.16.4",
    "express-fileupload": "1.1.3-alpha.2",
    "moment": "^2.24.0",
    "morgan": "1.9.1",
    "mysql2": "1.6.5",
    "nodemailer": "5.1.1",
    "nodemon": "1.18.10",
    "path": "0.12.7",
    "sequelize": "^4.37.6",
    "uuid": "3.3.2",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/register": "7.4.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "eslint": "5.8.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "5.2.0",
    "prettier": "1.16.4",
    "sequelize-cli": "5.5.0"
  }
}
